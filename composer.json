{
	"name": "ilyaplot/yii2-psr-log-target",
	"description": "Yii 2 log target which uses PSR-3 compatible logger",
	"keywords": ["yii", "extension", "log", "psr-3"],
	"homepage": "https://github.com/ilyaplot/yii2-psr-log-target",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/ilyaplot/yii2-psr-log-target/issues",
		"source": "https://github.com/ilyaplot/yii2-psr-log-target"
	},
	"authors": [
		{
			"name": "Alexander Makarov",
			"email": "sam@rmcreative.ru"
		},
		{
			"name": "Ilya Plotnikov",
			"email": "ilyaplot@gmail.com"
		}
	],
	"require": {
		"yiisoft/yii2": "~2.0.13",
        "psr/log": "~1.0.2"
	},
	"require-dev": {
        "phpunit/phpunit": "~4.4"
    },
	"autoload": {
		"psr-4": {
			"samdark\\log\\": "src",
			"samdark\\log\\tests\\": "tests"
		}
	}
}
